// Simple class with constructor and method.

class Duck {
    init(name) {
        this.name = name
    }

    quack() {
        print concat [this.name, "quacks"]
    }
}

class Math {
    square(n) {
        return n * n
    }
}

var duck = Duck("Waddles")
duck.quack()

var math = Math()
print math.square(3)