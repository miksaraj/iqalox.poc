# Simple for loop
for (var i = 1; i < 5; ++i) {
    print i * i
}

var fact = 1

# Another version of a simple for loop
for (var j = 1; j <= 10; ++j) {
    fact = fact * j
    concat ["Factorial of ", j, "is: ", fact] |> print
}

# Loop with continue and break
for (var k = 0; true; ++k) {
    (k == 0 or k == 5) ? continue : (k == 10) ? break : print k
}
