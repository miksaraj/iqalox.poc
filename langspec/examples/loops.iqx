// Simple for loop
for (var i = 1; i < 5; ++i) {
    print i * i
}

var j = 0

// Simple while loop
while (j < 10) {
    print j
    ++j
}

var fact = 1

// Another version of a simple for loop
for (var k = 1; k <= 10; ++k) {
    fact = fact * k
    concat ["Factorial of ", k, "is: ", fact] |> print
}

// Loop with continue and break
for (var l = 0; true; ++l) {
    (l == 0 or l == 5) ? continue : (l == 10) ? break : print l
}